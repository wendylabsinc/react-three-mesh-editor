{
  "name": "@wendylabsinc/react-three-mesh-editor",
  "version": "0.0.1",
  "description": "A react-three-fiber library for mesh editing similar to Blender with object/edit modes, vertex/edge/face selection",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "docs": "typedoc",
    "storybook": "storybook dev -p 6009",
    "build-storybook": "storybook build",
    "build-site": "npm run docs && npm run build-storybook -- -o site/stories && cp public/index.html site/index.html",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "three",
    "react-three-fiber",
    "mesh-editor",
    "3d",
    "blender",
    "vertices",
    "edges",
    "faces"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@react-three/drei": ">=9.0.0",
    "@react-three/fiber": ">=8.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "three": ">=0.150.0"
  },
  "devDependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@react-three/drei": "^9.92.0",
    "@react-three/fiber": "^8.15.0",
    "@storybook/addon-docs": "^8.4.0",
    "@storybook/addon-essentials": "^8.4.0",
    "@storybook/addon-interactions": "^8.4.0",
    "@storybook/addon-links": "^8.4.0",
    "@storybook/blocks": "^8.4.0",
    "@storybook/react": "^8.4.0",
    "@storybook/react-vite": "^8.4.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lucide-react": "^0.555.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.4.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.0.0",
    "three": "^0.160.0",
    "tsup": "^8.0.0",
    "typedoc": "^0.28.15",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
